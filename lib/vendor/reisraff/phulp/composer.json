{
  "name": "reisraff/phulp",
  "description": "The task manager for PHP",
  "type": "library",
  "keywords": [
    "phulp",
    "php"
  ],
  "license": "MIT",
  "homepage": "https://reisraff.github.io/phulp/#!/home",
  "authors": [
    {
      "name": "Rafael Reis",
      "email": "reisraff@gmail.com"
    }
  ],
  "support": {
    "issues": "https://github.com/reisraff/phulp/issues",
    "wiki": "https://github.com/reisraff/phulp/wiki",
    "source": "https://github.com/reisraff/phulp"
  },
  "require": {
    "php": ">=5.6",
    "symfony/finder": "^3.0",
    "react/event-loop": "~0.4",
    "doctrine/collections": "^1.3"
  },
  "require-dev": {
    "phing/phing": "2.*",
    "phpunit/phpunit": "~5.4",
    "squizlabs/php_codesniffer": "2.0.*@dev"
  },
  "autoload": {
    "psr-4": {
      "Phulp\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Phulp\\Test\\": "test"
    }
  },
  "config": {
    "platform": {
      "php": "5.6"
    },
    "bin-dir": "bin"
  },
  "bin": [
    "bin/phulp"
  ]
}
